{
  "name": "illuminate/redis",
  "description": "The Illuminate Redis package.",
  "license": "MIT",
  "homepage": "https://vdes.com",
  "support": {
    "issues": "https://github.com/vdes/framework/issues",
    "source": "https://github.com/vdes/framework"
  },
  "authors": [
    {
      "name": "Vitech Asia",
      "email": "vitech@vdes.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/collections": "^8.0",
    "illuminate/contracts": "^8.0",
    "illuminate/macroable": "^8.0",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Illuminate\\Redis\\": ""
    }
  },
  "suggest": {
    "ext-redis": "Required to use the phpredis connector (^4.0|^5.0).",
    "predis/predis": "Required to use the predis connector (^1.1.9)."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
