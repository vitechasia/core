{
  "name": "illuminate/collections",
  "description": "The Illuminate Collections package.",
  "license": "MIT",
  "homepage": "https://vdes.com",
  "support": {
    "issues": "https://github.com/vdes/framework/issues",
    "source": "https://github.com/vdes/framework"
  },
  "authors": [
    {
      "name": "Vitech Asia",
      "email": "vitech@vdes.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/contracts": "^8.0",
    "illuminate/macroable": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Illuminate\\Support\\": ""
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.x-dev"
    }
  },
  "suggest": {
    "symfony/var-dumper": "Required to use the dump method (^5.4)."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
